import java.util.Scanner;

class Main {
  public static void main(String[] args) {

    Scanner ler = new Scanner(System.in);

    int a;
    int result;
    
    System.out.printf("Digite um numero:\n");
    a = ler.nextInt();

    result = a * a;

    System.out.printf("O quadrado do número é:\t" + result);
    
    
  }
}
